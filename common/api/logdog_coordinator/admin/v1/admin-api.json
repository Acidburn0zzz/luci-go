{
 "kind": "discovery#restDescription",
 "discoveryVersion": "v1",
 "protocol": "rest",
 "id": "admin:v1",
 "name": "admin",
 "version": "v1",
 "description": "LogDog Admin API",
 "baseUrl": "http://localhost:8080/api/admin/v1/",
 "basePath": "/api/admin/v1/",
 "rootUrl": "http://localhost:8080/api/",
 "servicePath": "admin/v1/",
 "schemas": {
  "SetConfigRequest": {
   "id": "SetConfigRequest",
   "type": "object",
   "properties": {
    "configPath": {
     "type": "string",
     "required": true
    },
    "configService": {
     "type": "string",
     "required": true
    },
    "configSet": {
     "type": "string",
     "required": true
    }
   }
  }
 },
 "methods": {
  "setConfig": {
   "id": "admin.setConfig",
   "path": "setconfig",
   "httpMethod": "POST",
   "description": "Set the instance's global configuration parameters.",
   "request": {
    "$ref": "SetConfigRequest",
    "parameterName": "resource"
   },
   "scopes": [
    "https://www.googleapis.com/auth/userinfo.email"
   ]
  }
 }
}