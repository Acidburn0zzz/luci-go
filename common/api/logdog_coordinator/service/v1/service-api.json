{
 "kind": "discovery#restDescription",
 "discoveryVersion": "v1",
 "protocol": "rest",
 "id": "service:v1",
 "name": "service",
 "version": "v1",
 "description": "LogDog Service API",
 "baseUrl": "http://localhost:8080/api/service/v1/",
 "basePath": "/api/service/v1/",
 "rootUrl": "http://localhost:8080/api/",
 "servicePath": "service/v1/",
 "schemas": {
  "GetConfigResponse": {
   "id": "GetConfigResponse",
   "type": "object",
   "properties": {
    "configPath": {
     "type": "string"
    },
    "configService": {
     "type": "string"
    },
    "configSet": {
     "type": "string"
    }
   }
  },
  "LoadStreamResponse": {
   "id": "LoadStreamResponse",
   "type": "object",
   "properties": {
    "descriptor": {
     "type": "string",
     "format": "byte"
    },
    "path": {
     "type": "string"
    },
    "secret": {
     "type": "string",
     "format": "byte"
    },
    "state": {
     "$ref": "LogStreamState"
    }
   }
  },
  "LogStreamState": {
   "id": "LogStreamState",
   "type": "object",
   "properties": {
    "archiveDataURL": {
     "type": "string"
    },
    "archiveIndexURL": {
     "type": "string"
    },
    "archiveStreamURL": {
     "type": "string"
    },
    "created": {
     "type": "string"
    },
    "protoVersion": {
     "type": "string"
    },
    "purged": {
     "type": "boolean"
    },
    "terminalIndex": {
     "type": "string",
     "format": "int64"
    },
    "updated": {
     "type": "string"
    }
   }
  },
  "RegisterStreamRequest": {
   "id": "RegisterStreamRequest",
   "type": "object",
   "properties": {
    "descriptor": {
     "type": "string",
     "format": "byte",
     "required": true
    },
    "path": {
     "type": "string",
     "required": true
    },
    "protoVersion": {
     "type": "string",
     "required": true
    },
    "secret": {
     "type": "string",
     "format": "byte",
     "required": true
    }
   }
  },
  "RegisterStreamResponse": {
   "id": "RegisterStreamResponse",
   "type": "object",
   "properties": {
    "path": {
     "type": "string"
    },
    "secret": {
     "type": "string",
     "format": "byte"
    },
    "state": {
     "$ref": "LogStreamState"
    }
   }
  },
  "TerminateStreamRequest": {
   "id": "TerminateStreamRequest",
   "type": "object",
   "properties": {
    "path": {
     "type": "string",
     "required": true
    },
    "secret": {
     "type": "string",
     "format": "byte",
     "required": true
    },
    "terminalIndex": {
     "type": "string",
     "format": "int64"
    }
   }
  }
 },
 "methods": {
  "GetConfig": {
   "id": "service.GetConfig",
   "path": "getConfig",
   "httpMethod": "GET",
   "description": "Load service configuration parameters.",
   "response": {
    "$ref": "GetConfigResponse",
    "parameterName": "resource"
   },
   "scopes": [
    "https://www.googleapis.com/auth/userinfo.email"
   ]
  },
  "LoadStream": {
   "id": "service.LoadStream",
   "path": "loadStream",
   "httpMethod": "GET",
   "description": "Loads log stream metadata.",
   "parameters": {
    "Path": {
     "type": "string",
     "location": "query"
    }
   },
   "response": {
    "$ref": "LoadStreamResponse",
    "parameterName": "resource"
   },
   "scopes": [
    "https://www.googleapis.com/auth/userinfo.email"
   ]
  },
  "RegisterStream": {
   "id": "service.RegisterStream",
   "path": "registerStream",
   "httpMethod": "PUT",
   "description": "Registers a log stream.",
   "request": {
    "$ref": "RegisterStreamRequest",
    "parameterName": "resource"
   },
   "response": {
    "$ref": "RegisterStreamResponse",
    "parameterName": "resource"
   },
   "scopes": [
    "https://www.googleapis.com/auth/userinfo.email"
   ]
  },
  "TerminateStream": {
   "id": "service.TerminateStream",
   "path": "terminateStream",
   "httpMethod": "PUT",
   "description": "Register a log stream's terminal index.",
   "request": {
    "$ref": "TerminateStreamRequest",
    "parameterName": "resource"
   },
   "scopes": [
    "https://www.googleapis.com/auth/userinfo.email"
   ]
  }
 }
}